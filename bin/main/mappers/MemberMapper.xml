<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.catchyourbook.Repository.MemberMapper">
    <select id="checkMemberByMemberIdAndMemberPw" resultType="Integer">
    	 SELECT COUNT(*) FROM MEMBER_INFO m WHERE m.MEMBER_ID=#{memberId} AND m.MEMBER_PW=#{memberPw}
    </select>
</mapper>